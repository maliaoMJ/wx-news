/* pages/index/index.wxss */
page{
  width:100%;
  height:100%;
}
.index{
  width:100%;
  height:100%;
  background:rgba(179,212,219,225);
  display: flex;
  flex-direction: column;
  color:#fff;
  align-items: center;
}
.index>image{
  width:160rpx;
  height:160rpx;
  border-radius: 160rpx;
  margin-top: 150rpx;
}
.index_title{
  margin-top: 30rpx;
  font-size: 22px;
}
.index_btn{
  margin-top: 480rpx;
  height:80rpx;
  width:120px;
  border:2px solid #fff;
  line-height: 80rpx;
  text-align: center;
  border-radius: 5px;
}